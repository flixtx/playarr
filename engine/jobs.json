{
  "jobs": [
    {
      "name": "processProvidersTitles",
      "jobHistoryName": "ProcessProvidersTitlesJob",
      "interval": "1h",
      "timeout": "10s",
      "description": "Fetch metadata from IPTV providers and match TMDB IDs",
      "schedule": "Every 1 hour"
    },
    {
      "name": "processMainTitles",
      "jobHistoryName": "ProcessMainTitlesJob",
      "interval": "5m",
      "timeout": "1m",
      "description": "Generate main titles from provider titles",
      "schedule": "Every 5 minutes",
      "skipIfOtherInProgress": ["processProvidersTitles"]
    },
    {
      "name": "purgeProviderCache",
      "jobHistoryName": "PurgeProviderCacheJob",
      "interval": "6h",
      "timeout": "1h",
      "description": "Purge cache for deleted providers",
      "schedule": "Every 6 hours"
    }
  ]
}

